#~ msgstr "<code> docpath: /home/youruser/PathToYourDocuments/</code>"

#~ msgid "checks in the current workspace."
#~ msgstr "überprüft den aktuellen Workspace"

#~ msgid "How to use &hoem; in your projects"
#~ msgstr "Wie nutzen Sie &hoem; in Ihren Projekten"

#~ msgid "<code> develpath: /home/youruser/YourProjectdirectory</code>"
#~ msgstr "<code> develpath: /home/youruser/YourProjectdirectory</code>"

